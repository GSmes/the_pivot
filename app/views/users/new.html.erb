<h1>Create New Account</h1>

<div class="col-md-4 col-md-offset-4">
<%= form_for(@user) do |f|  %>
  <%= f.label :username %>
  <%= f.text_field :username %> <br>

  <%= f.label :email %>
  <%= f.text_field :email %> <br>

  <%= f.label :password %>
  <%= f.password_field :password %> <br>

  <%= f.submit "Create Account"%>
<% end %>
</div>
